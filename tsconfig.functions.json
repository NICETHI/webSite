/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src/api",
    "module": "CommonJS",
    "target": "ES2017",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "pretty": true,
    "removeComments": true,
    "outDir": "./api",
    "sourceMap": false
  },
  "include": ["./src/api/**/*"],
  "exclude": []
}
